IDR_MENU MENU DISCARDABLE 
BEGIN
    POPUP "文件(&F)"
    BEGIN
        MENUITEM "设置为自动启动(&E)",          ID_ENABLE_AUTOSTART
        MENUITEM "取消自动启动(&D)",            ID_DISABLE_AUTOSTART
        MENUITEM SEPARATOR
        MENUITEM "以系统服务方式自动启动(&I)",  ID_INSTALL_SERVICE
        MENUITEM "卸载系统服务取消自动启动(&R)", ID_REMOVE_SERVICE
        MENUITEM SEPARATOR
        MENUITEM "退出本程序(&x)",              ID_EXIT
    END
    POPUP "编辑(&E)"
    BEGIN
        MENUITEM "复制所有信息至剪贴板(&C)\tF10", ID_COPYALL
    END
    POPUP "工具与设置(&T)"
    BEGIN
        MENUITEM "重新加载系统信息(&R)\tF5",    ID_REFRESH
        MENUITEM SEPARATOR
        MENUITEM "配置文件系统驱动(&S)\tF7",    ID_SERVICE
        MENUITEM "性能及内存分配统计(&T)\tF8",  ID_PERFSTAT
        MENUITEM SEPARATOR
        MENUITEM "配置Ext2卷属性(&E)\tF3",      ID_CHANGE
        MENUITEM "盘符管理及设置(&M)\tF4",      ID_DRV_LETTER
        MENUITEM "删除无效盘符(&D)\tF9",        ID_REMOVE_DEAD_LETTER
        MENUITEM SEPARATOR
        MENUITEM "强制保存系统缓冲(&F)\tF11",   ID_FLUSH_BUFFER
        MENUITEM "更改分区类型ID(&C)\tF12",     ID_CHANGE_PARTTYPE
        MENUITEM SEPARATOR
        MENUITEM "显示磁盘或分区属性(&P)\tF6",  ID_PROPERTY
    END
    POPUP "帮助(&H)"
    BEGIN
        MENUITEM "帮助信息(&H)\tF1",            ID_HELP
        MENUITEM SEPARATOR
        MENUITEM "关于本程序(&A)\tF2",          ID_ABOUT
        MENUITEM SEPARATOR
        MENUITEM "支持我们...(&D)",             ID_DONATE
    END
END

